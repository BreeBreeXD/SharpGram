<?xml version="1.0" encoding="utf-8"?>
<Config>
    <Info>
        <Name>Pikore</Name>
		<Authorize>https://instagram.com/oauth/authorize/?client_id=[ID]&amp;redirect_uri=[RedirectUri]&amp;response_type=[ResponseType]&amp;scope=[Scope]</Authorize>
		<ClientID>2b5a0c10371c4784935b03e5619e94ca</ClientID>
		<RedirectUri>http://www.pikore.com/session/create?from=</RedirectUri>
		<ResponseType>code</ResponseType>
		<Scope>likes+comments+relationships</Scope>
		<MaxReq>aaa</MaxReq>
    </Info>
	
    <Link>
        <Like>http://www.pikore.com/m/like/[MediaID][_UserID]</Like>
        <Follow>http://www.pikore.com/follow/[UserID]</Follow>
        <Comment></Comment>
        <Feed>http://www.pikore.com/</Feed>
    </Link>
	
    <Header>
        <Host>www.pikore.com</Host>
        <Accept>*/*</Accept>
        <Accept-Language>it-IT,it;q=0.8,en-US;q=0.5,en;q=0.3</Accept-Language>
        <Accept-Encoding>gzip, deflate</Accept-Encoding>
        <ContentType>ContentType</ContentType>
        <X-Requested-With>XMLHttpRequest</X-Requested-With>
        <Pragma></Pragma>
        <Cache-Control></Cache-Control>
    </Header>
	
    <reqData>
        <Like>[GET]</Like>
        <Follow>[GET]</Follow>
        <Comment>NO_COMMENT</Comment>
    </reqData>
	
    <responseData>
        <Like>200</Like>
        <Follow>200</Follow>
        <Comment></Comment>
    </responseData>
</Config>